#: packages/redirect/controller.php:21
#: packages/redirect/blocks/redirect/controller.php:116
msgid "Redirect"
msgstr "Redirect"

#: packages/redirect/controller.php:26
msgid "This package offers a block to redirect users."
msgstr "Questo pacchetto offre un blocco per fare il redirect degli utenti."

#: packages/redirect/blocks/redirect/controller.php:126
msgid "Redirect specific users to another page."
msgstr "Reindirizza specifici utenti verso un’altra pagina."

#: packages/redirect/blocks/redirect/controller.php:163
msgid "No data received"
msgstr "Nessun dato ricevuto"

#: packages/redirect/blocks/redirect/controller.php:172
#: packages/redirect/blocks/redirect/controller.php:183
msgid "Please specify the destination page"
msgstr "Specificare la pagina di destinazione"

#: packages/redirect/blocks/redirect/controller.php:176
#: packages/redirect/blocks/redirect/controller.php:189
msgid "The destination page is the current page."
msgstr "La pagina di destinazione è la pagina corrente."

#: packages/redirect/blocks/redirect/controller.php:195
msgid "Please specify the kind of the destination page"
msgstr "Specificare il tipo della pagina di destinazione"

#: packages/redirect/blocks/redirect/controller.php:219
#, php-format
msgid "Invalid IP address: %s"
msgstr "Indirizzo IP non valido: %s"

#: packages/redirect/blocks/redirect/controller.php:237
msgid "Please specify if the message should be shown"
msgstr "Specificare se il messaggio va mostrato"

#: packages/redirect/blocks/redirect/controller.php:443
msgid "Redirect block"
msgstr "Blocco redirect"

#: packages/redirect/blocks/redirect/controller.php:457
msgid "This block will redirect selected users."
msgstr "Questo blocco reindirizzerà gli utenti selezionati."

#: packages/redirect/blocks/redirect/edit.php:7
msgid "Destination page"
msgstr "Pagina di destinazione"

#: packages/redirect/blocks/redirect/edit.php:8
msgid "Redirect by user groups"
msgstr "Reindirizza per gruppi di utenti"

#: packages/redirect/blocks/redirect/edit.php:9
msgid "Redirect by IP address"
msgstr "Reindirizza per indirizzo IP"

#: packages/redirect/blocks/redirect/edit.php:10
msgid "Options"
msgstr "Opzioni"

#: packages/redirect/blocks/redirect/edit.php:21
msgid "Please select"
msgstr "Selezionare"

#: packages/redirect/blocks/redirect/edit.php:24
msgid "Another page"
msgstr "Un’altra pagina"

#: packages/redirect/blocks/redirect/edit.php:28
msgid "External URL"
msgstr "URL esterno"

#: packages/redirect/blocks/redirect/edit.php:37
msgid "Choose page"
msgstr "Selezionare la pagina"

#: packages/redirect/blocks/redirect/edit.php:41
msgid "URL"
msgstr "URL"

#: packages/redirect/blocks/redirect/edit.php:59
msgid "Redirect members of these groups"
msgstr "Reindirizza i membri di questi gruppi"

#: packages/redirect/blocks/redirect/edit.php:60
msgid "Never redirect members of these groups"
msgstr "Non reindirizzare mai i membri di questi gruppi"

#: packages/redirect/blocks/redirect/edit.php:80
#: packages/redirect/blocks/redirect/edit.php:85
msgid "Select group"
msgstr "Seleziona gruppo"

#: packages/redirect/blocks/redirect/edit.php:144
msgid "Redirect these IP addresses"
msgstr "Reindirizza questi indirizzi IP"

#: packages/redirect/blocks/redirect/edit.php:145
msgid "Never redirect these IP addresses"
msgstr "Non reindirizzare mai questi indirizzi IP"

#: packages/redirect/blocks/redirect/edit.php:159
#, php-format
msgid "Separate multiple values by spaces, new lines or commas.<br />IPv4 and IPv6 addresses are supported.<br />You can specify single IP addresses as well as ranges (examples: %s)"
msgstr "Separa più valori con spazi, ritorni a capo o virgole.<br />È possibile specificare indirizzi IPv4 e IPv6.<br />È possibile specificare indirizzi IP singoli o intervalli di indirizzi (esempi: %s)"

#: packages/redirect/blocks/redirect/edit.php:176
msgid "Redirect users with permission to edit the page contents"
msgstr "Reindirizza gli utenti che possono modificare il contenuto della pagina"

#: packages/redirect/blocks/redirect/edit.php:181
msgid "Show block message"
msgstr "Mostra messaggio del blocco"

#: packages/redirect/blocks/redirect/edit.php:185
msgid "Never"
msgstr "Mai"

#: packages/redirect/blocks/redirect/edit.php:186
msgid "Only for users that can edit the page contents"
msgstr "Solo agli utenti che possono modificare il contenuto della pagina"

#: packages/redirect/blocks/redirect/edit.php:187
msgid "Always"
msgstr "Sempre"
